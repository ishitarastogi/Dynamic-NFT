{
  "name": "gelato-nft-web3-function",
  "version": "0.0.1",
  "description": "Create Hello World tasks using Gelato Ops SDK",
  "scripts": {
    "format": "prettier --write '*/**/*.*{js,json,md,ts}'",
    "format:check": "prettier --check '*/**/*.*{js,json,md,ts}'",
    "lint": "eslint -c .eslintrc.json --ext \"**/*.ts\"",
    "deploy": "npx hardhat run scripts/deploy-contract.ts",
    "w3f:deploy": "npx w3f deploy src/web3-functions/stable-diffusion-nft/index.ts",
    "test": "npx w3f test src/web3-functions/stable-diffusion-nft/index.ts --logs --chain-id=137",
    "test:batch": "npx w3f test web3-functions/stable-diffusion-nft/batch-reveal.ts --user-args=nftAddress:0x5041c60C75633F29DEb2AED79cB0A9ed79202415 --show-logs"
  },
  "license": "ISC",
  "devDependencies": {
    "@ethersproject/providers": "5.7.2",
    "@gelatonetwork/automate-sdk": "^2.7.0-beta",
    "@gelatonetwork/web3-functions-sdk": "2.1.7",
    "@nomicfoundation/hardhat-chai-matchers": "1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomiclabs/hardhat-ethers": "2.2.3",
    "@openzeppelin/contracts-upgradeable": "4.8.3",
    "@tsconfig/recommended": "1.0.2",
    "@typechain/ethers-v5": "^10.2.1",
    "@typechain/hardhat": "6.1.6",
    "@types/chance": "1.1.3",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.7.10",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "chai": "4.3.7",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "ethers": "5.7.2",
    "hardhat": "2.14.0",
    "hardhat-contract-sizer": "2.8.0",
    "hardhat-deploy": "0.11.29",
    "husky": "8.0.3",
    "lint-staged": "11.1.2",
    "nft.storage": "^7.0.0",
    "prettier": "2.8.8",
    "prettier-plugin-solidity": "1.1.3",
    "rxjs": "7.8.1",
    "solhint": "3.4.1",
    "solhint-plugin-prettier": "0.0.5",
    "ts-node": "10.9.1",
    "typechain": "^8.1.1",
    "typescript": "5.0.4"
  },
  "lint-staged": {
    "*.*{js,json,md,ts,yml,yaml}": "prettier --write",
    "*.*{ts,js}": "eslint -c .eslintrc.json"
  },
  "dependencies": {
    "@gelatonetwork/ops-sdk": "^2.0.8",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@openzeppelin/contracts": "^4.9.3"
  }
}
